{
  "name": "datetime-attribute",
  "version": "1.3.1",
  "description": "Generate datetime attributes for the <time> HTML element, following WHATWG and ISO8601 specs.",
  "type": "module",
  "sideEffects": false,
  "exports": "./src/index.js",
  "engines": {
    "node": ">=12.8"
  },
  "packageManager": "pnpm@7.27.0",
  "scripts": {
    "types": "tsc",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test-types": "pnpm exec tsd",
    "coverage": "vitest run --coverage",
    "watch": "vitest watch",
    "lint": "pnpm exec eslint src/**/*.js tests/index.test.js",
    "lint-fix": "pnpm exec eslint src/**/*.js tests/index.test.js --fix"
  },
  "files": [
    "CHANGELOG.md",
    "src/**/*.js",
    "types/**/*.d.ts"
  ],
  "types": "./types/index.d.ts",
  "keywords": [
    "time",
    "datetime",
    "attribute",
    "date",
    "duration",
    "iso8601"
  ],
  "homepage": "https://github.com/meduzen/datetime-attribute",
  "author": "Mehdi Merah",
  "license": "WTFPL",
  "repository": "https://github.com/meduzen/datetime-attribute.git",
  "bugs": {
    "url": "https://github.com/meduzen/datetime-attribute/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.32.4",
    "@vitest/ui": "^0.32.4",
    "eslint": "^8.44.0",
    "tsd": "^0.28.1",
    "typescript": "^5.1.6",
    "vitest": "^0.32.4"
  }
}
