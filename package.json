{
  "name": "datetime-attribute",
  "version": "1.3.1",
  "description": "Generate datetime attributes for the <time> HTML element, following WHATWG and ISO8601 specs.",
  "type": "module",
  "sideEffects": false,
  "exports": "./src/index.js",
  "engines": {
    "node": ">=12.8"
  },
  "scripts": {
    "types": "tsc src/index.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "test": "vitest run",
    "test-types": "npx tsd",
    "coverage": "vitest run --coverage",
    "watch": "vitest watch",
    "lint": "npx eslint src/**/*.js tests/index.test.js",
    "lint-fix": "npx eslint src/**/*.js tests/index.test.js --fix"
  },
  "files": [
    "CHANGELOG.md",
    "src/**/*.js",
    "types/**/*.d.ts"
  ],
  "types": "./types/index.d.ts",
  "keywords": [
    "time",
    "datetime",
    "attribute",
    "date",
    "duration",
    "iso8601"
  ],
  "homepage": "https://github.com/meduzen/datetime-attribute",
  "author": "Mehdi Merah",
  "license": "WTFPL",
  "repository": "https://github.com/meduzen/datetime-attribute.git",
  "bugs": {
    "url": "https://github.com/meduzen/datetime-attribute/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.28.2",
    "eslint": "^8",
    "tsd": "^0.25",
    "typescript": "^4.9.4",
    "vitest": "^0.28.2"
  }
}
